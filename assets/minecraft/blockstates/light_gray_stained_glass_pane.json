{
  "multipart": [
    {
      "when": {
        "north": true
      },
      "apply": {
        "model": "block/glass_pane_half_right_silver"
      }
    },
    {
      "when": {
        "east": true
      },
      "apply": {
        "model": "block/glass_pane_half_right_silver",
        "y": 90
      }
    },
    {
      "when": {
        "south": true
      },
      "apply": {
        "model": "block/glass_pane_half_left_silver"
      }
    },
    {
      "when": {
        "west": true
      },
      "apply": {
        "model": "block/glass_pane_half_left_silver",
        "y": 90
      }
    },
    {
      "when": {
        "north": false,
        "east": false,
        "south": false,
        "west": false
      },
      "apply": [
        { "model": "mcme:block/oak_thin_trunk_vertical" },
        { "model": "mcme:block/oak_thin_trunk_vertical_2" },
        { "model": "mcme:block/oak_thin_trunk_vertical_3" },
        { "model": "mcme:block/oak_thin_trunk_vertical_4" }
    ]
    }
  ]
}
