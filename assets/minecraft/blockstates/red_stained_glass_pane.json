{
  "multipart": [
    {
      "when": {
        "north": true
      },
      "apply": {
        "model": "block/glass_pane_half_right_red"
      }
    },
    {
      "when": {
        "east": true
      },
      "apply": {
        "model": "block/glass_pane_half_right_red",
        "y": 90
      }
    },
    {
      "when": {
        "south": true
      },
      "apply": {
        "model": "block/glass_pane_half_left_red"
      }
    },
    {
      "when": {
        "west": true
      },
      "apply": {
        "model": "block/glass_pane_half_left_red",
        "y": 90
      }
    },
    {
      "when": {
        "OR": [
          {
            "north": false,
            "south": true
          },
          {
            "north": true,
            "south": false
          },
          {
            "west": false,
            "east": true
          },
          {
            "west": true,
            "east": false
          }
        ]
      },
      "apply": {
        "model": "block/glass_pane_center_red"
      }
    },
    {
      "when": {
        "north": false,
        "east": false,
        "south": false,
        "west": false
      },
      "apply": {
        "model": "block/glass_pane_center_red"
      }
    }
  ]
}
