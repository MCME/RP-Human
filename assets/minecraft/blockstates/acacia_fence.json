{
  "multipart": [
    {
      "when": {
        "north": "false",
        "east": "false",
        "south": "false",
        "west": "false"
      },
      "apply": {
        "model": "mcme:block/oak_thin_trunk_vertical"
      }
    },
    {
      "when": {
        "OR": [
          {
            "north": "true"
          }
        ]
      },
      "apply": {
        "model": "block/oak_thin_trunk_horizontal"
      }
    },
    {
      "when": {
        "OR": [
          {
            "east": "true"
          }
        ]
      },
      "apply": {
        "model": "block/oak_thin_trunk_horizontal_2"
      }
    },
    {
      "when": {
        "OR": [
          {
            "south": "true"
          }
        ]
      },
      "apply": {
        "model": "block/oak_thin_trunk_horizontal_3"
      }
    },
    {
      "when": {
        "OR": [
          {
            "west": "true"
          }
        ]
      },
      "apply": {
        "model": "block/oak_thin_trunk_horizontal_4"
      }
    },
    {
      "when": {
        "north": "true",
        "east": "true",
        "south": "false",
        "west": "false"
      },
      "apply": {
        "model": "block/tree_thin_oak_center_alt"
      }
    },
    {
      "when": {
        "north": "false",
        "east": "true",
        "south": "true",
        "west": "false"
      },
      "apply": {
        "model": "block/tree_thin_oak_center_alt"
      }
    },
    {
      "when": {
        "north": "false",
        "east": "false",
        "south": "true",
        "west": "true"
      },
      "apply": {
        "model": "block/tree_thin_oak_center_alt"
      }
    },
    {
      "when": {
        "north": "true",
        "east": "false",
        "south": "false",
        "west": "true"
      },
      "apply": {
        "model": "block/tree_thin_oak_center_alt"
      }
    }
  ]
}
