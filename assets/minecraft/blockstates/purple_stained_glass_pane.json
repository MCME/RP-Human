{
  "multipart": [
    {
      "when": {
        "north": true
      },
      "apply": {
        "model": "block/glass_pane_half_right_purple"
      }
    },
    {
      "when": {
        "east": true
      },
      "apply": {
        "model": "block/glass_pane_half_right_purple",
        "y": 90
      }
    },
    {
      "when": {
        "south": true
      },
      "apply": {
        "model": "block/glass_pane_half_left_purple"
      }
    },
    {
      "when": {
        "west": true
      },
      "apply": {
        "model": "block/glass_pane_half_left_purple",
        "y": 90
      }
    },
    {
      "when": {
        "north": false,
        "east": false,
        "south": false,
        "west": false
      },
      "apply": [
        { "model": "mcme:block/mist"  },
        { "model": "mcme:block/mist", "y": 90  },
        { "model": "mcme:block/mist", "y": 180  },
        { "model": "mcme:block/mist", "y": 270  },
        { "model": "mcme:block/mist_2"  },
        { "model": "mcme:block/mist_2", "y": 90  },
        { "model": "mcme:block/mist_2", "y": 180  },
        { "model": "mcme:block/mist_2", "y": 270  },
        { "model": "mcme:block/mist_3"  },
        { "model": "mcme:block/mist_3", "y": 90  },
        { "model": "mcme:block/mist_3", "y": 180  },
        { "model": "mcme:block/mist_3", "y": 270  }	
        ]
    }
  ]
}
